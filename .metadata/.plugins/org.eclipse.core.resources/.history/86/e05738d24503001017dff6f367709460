#define PERIPH_BASE 				(0x40000000UL)
#define AHB1_PEPRIPH_OFFSET			(0x00020000UL)
#define AHB1_PERIPH_BASE			(PERIPH_BASE + AHB1_PERIPH_OFFSET)
#define GPIOA_OFFSET 				(0x0000UL)
#define GPIOA_BASE					(AHB1_PERIPH_BASE + GPIOA_OFFSET)
#define RCC_OFFSET					(0x00003800UL)
#define RCC_BASE					(AHB1_PERIPH_BASE + RCC_OFFSET)

#define AHB1EN_R_OFFSET				(0X30UL)
#define RCC_AHB1EN_R			    (RCC_BASE + AHB1EN_R_OFFSET)

#define MODE_R_OFFSET				(0x0UL)
#define GPIOA_MODE_R				(GPIOA_BASE + MODE_R_OFFSET)

#define ODR_R_OFFSET				(0x14UL)
#define GPIOA_ODR					(GPIOA_BASE + ODR_R_OFFSET)

#define GPIOAEN						(1U<<0)
