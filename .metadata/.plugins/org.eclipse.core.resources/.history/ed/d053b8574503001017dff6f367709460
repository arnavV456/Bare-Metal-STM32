#define PERIPH_BASE 				0x40000000UL
#define AHB1_PEPRIPH_OFFSET			0x00020000UL
#define AHB1_PERIPH_BASE			PERIPH_BASE + APB1_PERIPH_OFFSET
#define GPIOA_OFFSET 				0x0000U
#define GPIOA_BASE					APB1_PERIPH_BASE + GPIOA_OFFSET
#define RCC_OFFSET					0x00003800UL
#define RCC_BASE					APB1_PERIPH_BASE + RCC_OFFSET

#define RCC_AHB1_ENABLE				RCC_BASE + 0x30
